{
  "name": "fsspx-hub",
  "version": "0.1.0",
  "maintainers": [
    "jacek@adamczyk.me"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "dev:email": "email dev --dir src/emails",
    "download-fonts": "node ./scripts/download-fonts.cjs",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "ci": "payload migrate && pnpm build",
    "deploy": "pnpm run deploy:database && pnpm run deploy:app",
    "deploy:app": "opennextjs-cloudflare build --env=$CLOUDFLARE_ENV && opennextjs-cloudflare deploy --env=$CLOUDFLARE_ENV",
    "deploy:database": "cross-env NODE_ENV=production PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute fsspx-hub-db --command 'PRAGMA optimize' --env=$CLOUDFLARE_ENV --remote",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env=$CLOUDFLARE_ENV",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "pnpm download-fonts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-sesv2": "^3.916.0",
    "@marsidev/react-turnstile": "^1.3.1",
    "@opennextjs/cloudflare": "^1.11.0",
    "@payloadcms/db-d1-sqlite": "3.67.0",
    "@payloadcms/email-nodemailer": "3.67.0",
    "@payloadcms/next": "3.67.0",
    "@payloadcms/plugin-form-builder": "3.67.0",
    "@payloadcms/plugin-import-export": "3.67.0",
    "@payloadcms/richtext-lexical": "3.67.0",
    "@payloadcms/storage-s3": "3.67.0",
    "@payloadcms/translations": "3.67.0",
    "@payloadcms/ui": "3.67.0",
    "@posthog/nextjs-config": "^1.4.0",
    "@react-email/components": "0.3.2",
    "@react-email/render": "^1.4.0",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/nodemailer": "^7.0.3",
    "base62": "^2.0.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "escape-html": "^1.0.3",
    "graphql": "^16.8.1",
    "html-minifier-terser": "^7.2.0",
    "lucide-react": "^0.562.0",
    "mime-types": "^3.0.2",
    "next": "^15.5.7",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "payload": "3.67.0",
    "photoswipe": "^5.4.4",
    "posthog-js": "^1.294.0",
    "posthog-node": "^5.9.5",
    "react": "^19.2.1",
    "react-calendar": "^5.1.0",
    "react-dom": "^19.2.1",
    "react-photoswipe-gallery": "^4.0.0",
    "react-tooltip": "^5.28.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/nextjs": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/escape-html": "^1.0.4",
    "@types/jest": "^29.5.11",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20",
    "@types/react": "19.1.6",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "19.1.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "15.5.7",
    "eslint-plugin-storybook": "^0.11.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "react-email": "4.0.8",
    "storybook": "^8.5.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "uglify-js": "3.19.3",
    "wrangler": "^3.0.0"
  },
  "peerDependencies": {
    "uglify-js": "3.19.3"
  }
}
