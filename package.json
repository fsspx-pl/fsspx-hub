{
  "name": "my-app",
  "version": "0.1.0",
  "maintainers": [
    "jacek@adamczyk.me"
  ],
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "dev:email": "email dev --dir src/emails",
    "download-fonts": "node ./scripts/download-fonts.cjs",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "node ./src/scripts/standalone-script.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "pnpm download-fonts"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/translations": "3.37.0",
    "@payloadcms/ui": "3.37.0",
    "@react-email/components": "0.3.2",
    "@react-email/render": "1.0.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "escape-html": "^1.0.3",
    "graphql": "^16.8.1",
    "next": "^15.4.2",
    "payload": "3.37.0",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^19.0.0",
    "react-tooltip": "^5.28.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/nextjs": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^20",
    "@types/react": "19.1.6",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "19.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-storybook": "^0.11.2",
    "postcss": "^8",
    "react-email": "4.2.3",
    "storybook": "^8.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.5"
  }
}
