{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PayloadCMSMediaStorageProd",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::fsspx-hub-prod/media/*",
        "arn:aws:s3:::fsspx-hub-prod/uploads/*"
      ]
    },
    {
      "Sid": "PayloadCMSBucketListProd",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::fsspx-hub-prod",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "media/*",
            "uploads/*"
          ]
        }
      }
    }
  ]
}
